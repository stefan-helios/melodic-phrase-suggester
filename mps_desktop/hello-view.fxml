<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.melphrasuggui.HelloController">
   <children>
      <ListView fx:id="phraseListView" onKeyPressed="#onListViewClick" onMouseClicked="#onListViewClick" prefWidth="600.0" />
      <TextField fx:id="currentPhraseTextField" onKeyPressed="#onTextFieldAction" />
      <Button fx:id="generateButton" mnemonicParsing="false" onAction="#onGenerateButtonClick" text="Generate new phrase" />
      <Button fx:id="playButton" mnemonicParsing="false" onAction="#onPlayButtonClick" text="Play current phrase" />
      <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Save current phrase" />
      <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Delete current phrase" />
      <Button fx:id="quitButton" mnemonicParsing="false" onAction="#onQuitButtonClick" text="Quit" />
      <Label fx:id="messageLabel" prefHeight="134.0" prefWidth="590.0" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
